<zk xmlns:n="native" xmlns:w="client">
	<div width="80%" class="container ${arg.animation_}" use="karen.core.form.controllers.C_WindowForm" apply="org.zkoss.bind.BindComposer" viewModel="@id('vm') @init('ve.smile.datos.configuracion.VMVPlantillaEvento')">

		<div class="row">
			<div class="col s12">
				<div class="input-field col s12">
					<textbox id="nombre" type="text" value="@bind(vm.evento.nombre)" readonly="true"> </textbox>
				</div>
			</div>
			
			<div>
				<tabbox id="tb">
    				<tabs id="tabs">
        				<tab id="tabIndicadores" label="Indicadores" />
        				<tab id="tabTareas" label="Tareas" />
    				</tabs>
    			
    				<tabpanels>
        				<tabpanel>
							<hlayout>
								<vlayout>
									Indicadores
									<listbox model="@bind(vm.indicadores)" selectedItems="@bind(vm.indicadoresSel)" multiple="true" width="400px" height="300px"/>
										<template name="model">
											<listitem label="@load(each.nombre)" />
										</template>
								</vlayout>
								<vbox vflex="1">
									<button label="&gt;" onClick="@command('indicadoresAPlantilla')"/>
									<button label="&lt;" onClick="@command('indicadoresFueraPlantilla')"/>
								</vbox>
								<vlayout>
									Indicadores del evento
									<listbox model="@bind(vm.plantillaEventoIndicadores)" selectedItems="@bind(vm.plantillaEventoIndicadoresSel)" multiple="true" width="400px" height="300px"/>
										<template name="model">
											<listitem label="@load(each.fkIndicador.nombre)" />
										</template>
								</vlayout>
							</hlayout>
        				</tabpanel>
        				<tabpanel>
							<hlayout>
								<vlayout>
									Tareas
									<listbox model="@bind(vm.tareas)" selectedItems="@bind(vm.tareasSel)" multiple="true" width="400px" height="300px"/>
										<template name="model">
											<listitem label="@load(each.nombre)" />
										</template>
								</vlayout>
								<vbox vflex="1" pack="middle">
									<button label="&gt;" onClick="@command('tareasAPlantilla')"/>
									<button label="&lt;" onClick="@command('tareasFueraPlantilla')"/>
								</vbox>
								<vlayout>
									Tareas del evento
									<listbox model="@bind(vm.plantillaEventoTareas)" selectedItems="@bind(vm.plantillaEventoTareasSel)" multiple="true" width="400px" height="300px"/>
										<template name="model">
											<listitem label="@load(each.fkTarea.nombre)" />
										</template>
								</vlayout>
							</hlayout>
        				</tabpanel>
    			</tabpanels>
			</tabbox>	
		</div>
			
						
		</div>
				
		<div id="divButtonsForm" use="karen.core.form.buttons.containers.DivButtonsForm">
		
		</div>

	</div>
</zk>
