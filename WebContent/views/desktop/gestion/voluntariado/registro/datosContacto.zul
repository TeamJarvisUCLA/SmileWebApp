<zk xmlns:n="native" xmlns:w="client">
	<div sclass="animated fadeInLeft">
		<div class="row">
			<div class="col s12" align="center" style="margin-bottom: 15px;">
				<label value="Ingrese la información de contacto del voluntario" sclass="label-medium-500"></label>
			</div>
		</div>
		
		<div width="100%">
			<div class="row">
				<div class="col s12">
					
					<div class="input-field col s6">
						<n:label for="telefono_1" class="active">Teléfono 1</n:label>
						<textbox id="telefono_1" type="text" value="@bind(vm.selectedObject.fkPersona.telefono1)"> </textbox>
						<span sclass="fa fa-question-circle fa-lg help-icon-special" tooltip="pop_telefono1, position=after_end" />
					</div>
					
					<popup id="pop_telefono1" width="200px">
        				<html>
        					<![CDATA[Por favor, ingrese el número telefónico del voluntario que desea registrar.<br/>]]>
        				</html>
    				</popup>
				
					<div class="input-field col s6">
						<n:label for="telefono_2" class="active">Teléfono 2</n:label>
						<textbox id="telefono_2" type="text" value="@bind(vm.selectedObject.fkPersona.telefono2)"> </textbox>
						<span sclass="fa fa-question-circle fa-lg help-icon-special" tooltip="pop_telefono2, position=after_end" />
					</div>
					
					<popup id="pop_telefono2" width="200px">
        				<html>
        					<![CDATA[Por favor, ingrese el número telefónico del voluntario que desea registrar.<br/>]]>
        				</html>
    				</popup>
					
					<div class="input-field col s6">
						<n:label for="fax" class="active">Fax</n:label>
						<textbox id="fax" type="text" value="@bind(vm.selectedObject.fkPersona.fax)"> </textbox>
						<span sclass="fa fa-question-circle fa-lg help-icon-special" tooltip="pop_fax, position=after_end" />
					</div>
					
					<popup id="pop_fax" width="200px">
        				<html>
        					<![CDATA[Por favor, ingrese el número de fax del voluntario que desea registrar.<br/>]]>
        				</html>
    				</popup>					
					
					<div class="input-field col s6">
						<n:label for="correo" class="active">Correo electrónico</n:label>
						<textbox id="correo" type="text" value="@bind(vm.selectedObject.fkPersona.correo)"> </textbox>
						<span sclass="fa fa-question-circle fa-lg help-icon-special" tooltip="pop_correo, position=after_end" />
