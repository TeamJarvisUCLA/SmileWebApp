		<div sclass="col s12">
			<grid id="Grid" sclass="periodo grid" height="500px"
				emptyMessage="No hay actividades asociadas al trabajo social"
				model="@bind(vm.listTsPlanActividads)">
				<columns sclass="header-periodo-vacacional">
					<column width="5%" />
					<column label="Nombre" sort="auto(nombre)"
						width="25%" />
					<column label="Descripci贸n" sort="auto(descripcion)"
						align="center" width="35%" />
					<column label="Clasificador de Tarea"
						sort="auto(clasificadoTarea)" align="center" width="23%" />
					<column label="Recursos" align="center"
						width="12%">

					</column>
				</columns>
				<template name="model">
					<row style="overflow:auto; max-height:400px"
						sclass="row-vacaciones">


						<detail open="false" fulfill="onOpen">

							<grid sclass="vacacion"
								model="@bind(each.tsPlanActividadRecursos)">
								<auxhead>
									<auxheader colspan="6"
										align="center" label="RECURSOS ASOCIADOS">
									</auxheader>
								</auxhead>
								<columns
									sclass="header-periodo-disfute">
									<column label="Nombre" width="15%" />
									<column label="Descripci贸n"
										align="center" width="20%" />
									<column label="Unidad de Medida"
										align="center" width="20%" />
									<column label="Valor" align="center"
										width="15%">
									</column>
									<column label="Fecha de Asignaci贸n"
										align="center" width="20%">
									</column>
									<column label="eliminar"
										align="center" width="10%">
									</column>
								</columns>
								<template name="model">
									<row sclass="last">
										<cell sclass="vacacion">
											<label
												value="@bind(each.fkRecurso.nombre)" />
										</cell>
										<cell sclass="vacacion">
											<label
												value="@bind(each.fkRecurso.descripcion)" />
										</cell>
										<cell sclass="vacacion">
											<label
												value="@bind(each.fkRecurso.fkUnidadMedida.nombre)" />
										</cell>
										<cell sclass="vacacion">
											<intbox
												style="height: 1em; border: 1px solid #999999; border-radius: 7px; font-size:1.2em;  text-align: center;"
												value="@bind(each.cantidad)">
											</intbox>
										</cell>
										<cell>
											<datebox format="dd/MM/yyyy"
												value="@bind(each.fechaAsignacionDate)" width="190px">
											</datebox>
										</cell>
										<cell sclass="vacacion"
											style="    border-left: 2px solid #EEEEEE !important;">
											<button
												onClick="@command('eliminarPeriodoDisfrute', periodoDisfrute=each)"
												sclass="btn-sm-grid btn-pink " style="margin-left: 3px;"
												tooltiptext="Eliminar" iconSclass="fa fa-trash">
											</button>
										</cell>
									</row>
								</template>
							</grid>
						</detail>

						<cell sclass="periodo">
							<label value="@bind(each.fkTarea.nombre) " />
						</cell>

						<cell sclass="periodo">
							<label
								value="@bind(each.fkTarea.descripcion)" />
						</cell>
						<cell sclass="periodo">
							<label
								value="@bind(each.fkTarea.fkClasificadorTarea.nombre)" />
						</cell>
						<cell sclass="periodo">
							<button
								sclass="teal lighten-1 waves-effect waves-light"
								onClick="@command('buscarIndicadores', index=forEachStatus.index)"
								iconSclass="fa fa-search" tooltiptext="Editar Lugar"
								style="/padding: 5px 6px; background-color: #2AA4EF !important; float: center">
							</button>
