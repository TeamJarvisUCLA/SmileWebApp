 <?xel-method prefix="m" name="convertirLongATimestamp" class="lights.smile.util.UtilConverterDataList" 
	signature="java.lang.String convertirLongATimestamp(java.lang.Long)" ?> 
<zk xmlns:n="native" xmlns:w="client">
	<div sclass="animated fadeInLeft">
		<div class="row">
			<div class="col s12" align="center"
				style="margin-bottom: 15px;">
				<label value="Seleccione la Solicitud de Ayuda"
					sclass="label-medium-500">
				</label>
			</div>

		</div>
		<div width="100%">

			<div class="row">
				<div class="col s12">
					<div class="col s12">
						<listbox id="listbox"
							model="@load(vm.objectsList)"
							selectedItem="@bind(vm.selectedObject)">


							<listhead sizable="true">
							<listheader width="15%" label="Titulo de Solicitud" />
							<listheader width="20%" label="Estatus de Solicitud" />
							<listheader width="20%" label="Nombre de Beneficiario" />
							<listheader width="20%" label="Apellido de Beneficiario" />
							<!-- <listheader width="20%" label="Nombre de Representante" />
							<listheader width="20%" label="Apellido de Representante" />  -->
							<listheader width="10%" label="Urgencia" />
							<listheader width="10%" label="Fecha" />
							</listhead>
							<template name="model">
								<listitem>
								
								<listcell label="@load(each.titulo)" />		
								<listcell label="@load(each.estatusSolicitudEnum)" />
								<listcell label="@load(each.fkBeneficiario.fkPersona.nombre)" />
								<listcell label="@load(each.fkBeneficiario.fkPersona.apellido)" />
								<!-- <listcell label="@load(each.fkBeneficiario.fkPersona.nombre)" />
								<listcell label="@load(each.fkBeneficiario.fkPersona.nombre)" />  -->
								<listcell label="@load(each.urgenciaEnum)" />
								<listcell label="${m:convertirLongATimestamp(each.fecha)}" />  
									
								</listitem>
							</template>
							<listfoot>
								<listfooter id="listFoot" span="5"
									use="karen.core.listfoot.ListFoot">
								</listfooter>
							</listfoot>
						</listbox>
					</div>
				</div>
			</div>
		</div>
	</div>
</zk>
