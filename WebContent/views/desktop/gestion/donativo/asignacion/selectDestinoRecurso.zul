 <zk xmlns:n="native" xmlns:w="client">
	<div sclass="animated fadeInLeft">
		<div class="row">
			<div class="col s12" align="center"
				style="margin-bottom: 15px;">
				<label value="Seleccione el Destino del Recurso a Asignar"
					sclass="label-medium-500">
				</label>
			</div>

		</div>
		<div width="100%">

			<div class="row">
				<div class="col s12">
					<div class="col s12">
						<listbox 
							model="@load(vm.tipoDestinoRecurso)"
							selectedItem="@bind(vm.destinoRecurso)">

							<listhead sizable="true">
								<listheader width="100%"
									label="Recurso" />
							</listhead>
							<template name="model">
								<listitem>
									<listcell
										label="@load(each.tipoDestinoRecurso)" />
								</listitem>
							</template>
							
						</listbox>
					</div>
				</div>
			</div>
		</div>
	</div>
</zk>